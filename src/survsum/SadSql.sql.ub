   type t_type_code      is table of varchar2( 3)   index by binary_integer;
   type t_type_name      is table of varchar2(65)   index by binary_integer;
   type t_group_name     is table of varchar2(21)   index by binary_integer;
   type t_remarks        is table of varchar2(40)   index by binary_integer;
   type t_sample_id      is table of varchar2( 9)   index by binary_integer;
   type t_samples_code   is table of number(4)      index by binary_integer;
