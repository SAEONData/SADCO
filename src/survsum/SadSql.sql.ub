   type t_watnut_cnt     is table of number(10)     index by binary_integer;
   type t_watpol1_cnt    is table of number(10)     index by binary_integer;
   type t_watpol2_cnt    is table of number(10)     index by binary_integer;
   type t_watchem1_cnt   is table of number(10)     index by binary_integer;
   type t_watchem2_cnt   is table of number(10)     index by binary_integer;
   type t_watchl_cnt     is table of number(10)     index by binary_integer;
   type t_sedphy_cnt     is table of number(10)     index by binary_integer;
   type t_sedpes_cnt     is table of number(10)     index by binary_integer;
   type t_sedpol1_cnt    is table of number(10)     index by binary_integer;
   type t_sedpol2_cnt    is table of number(10)     index by binary_integer;
   type t_sedchem1_cnt   is table of number(10)     index by binary_integer;
   type t_sedchem2_cnt   is table of number(10)     index by binary_integer;
